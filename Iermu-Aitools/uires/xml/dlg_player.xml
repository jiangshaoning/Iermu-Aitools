    <window name="realplay_wnd" pos="2,[0,-2,-50">
      <realwnd pos="0,0,-0,-0" name="ffplaywnd"  focusable="0" wndclass="CRealWndDlg" wndname="mainWindow" />
    </window>
    <caption id="122" pos="0,-48,-2,-2">
      <imgbtn id="250" pos="40,20" size="16,30" cursor="hand"  tip="录屏" animate="1" skin="btn.record" />
      <imgbtn id="251" pos="[20,13" size="30,30" cursor="hand"  tip="抓拍" animate="1" skin="btn.snapshot" />
      <imgbtn data="1" id="206" pos="[10,16" cursor="hand" size="16,16" tip="静音" animate="1" skin="btn.volume_zero" />
      <sliderbar name="volumeSlider" thumbInRail="0" cursor="hand" width="100" value="50" min="0" max="100" pos="[0,15" thumbSkin="skin.SliderPlayvol_png" posSkin="skin.SliderForeVolume_png" bkgndSkin="skin.SliderBackVolume_png"  />
      <imgbtn id="600" pos="[10,15" size="24,18" cursor="hand" tip="全屏" animate="1" skin="btn.screen_full" />
    </caption>
   
  </root>
  
</SOUI>
